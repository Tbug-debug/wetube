extends base
include mixins/video

block upload
    nav
        ul 
            li 
                a(href="/") Home
            li 
                a(href="videos/upload") Upload


block content
    form(method="GET")
        input(placeholder="Search by title", name="keyword", type="text")
        input(type="submit",value="Search now")

    div
        each fish in videos 
            +video(fish)